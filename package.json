{
  "name": "react-redux-webpack",
  "version": "1.0.0",
  "description": "Building out basic RRW based app",
  "main": "server.js",
  "scripts": {
    "prebuild:js": "jshint **.js",
    "prebuild:css": "node-sass client/css/main.scss client/css/main.css",
    "build:css": "cssmin ./client/css/main.css > ./dist/main.min.css",
    "build:js": "webpack --config webpack.config.js",
    "serve": "npm run build:js  && npm run build:css && node server.js",
    "start": "nodemon \"npm run serve\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jon Lucas",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4",
    "mongoose": "^4.4.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "cssmin": "^0.4.3",
    "jshint": "^2.9.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "webpack": "^1.12.13"
  }
}
